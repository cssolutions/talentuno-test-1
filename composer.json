{
  "name": "eldahar/talentuno-test-1",
  "type": "project",
  "description": "Talentuno test 1",
  "require": {
    "php": "^7.2",
    "symfony/console": "*",
    "symfony/yaml": "*",
    "symfony/dependency-injection": "^4.2",
    "symfony/http-kernel": "^4.2",
    "symfony/config": "^4.2"
  },
  "config": {
    "platform": {
      "php": "7.2"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "4.3.*"
    }
  }
}